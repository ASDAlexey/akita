{
  "name": "akita",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "hmr": "ng serve --hmr",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "fix:prettier:html": "prettier -w \"./src/**/**.html\" --cache",
    "update-lock": "npm i --package-lock-only"
  },
  "dependencies": {
    "@angular/animations": "^15.2.8",
    "@angular/common": "^15.2.8",
    "@angular/compiler": "^15.2.8",
    "@angular/core": "^15.2.8",
    "@angular/forms": "^15.2.8",
    "@angular/platform-browser": "^15.2.8",
    "@angular/platform-browser-dynamic": "^15.2.8",
    "@angular/router": "^15.2.8",
    "@datorama/akita": "8.0.1",
    "@datorama/akita-ng-entity-service": "^8.0.0",
    "@datorama/akita-ng-router-store": "^8.0.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^43.1.1",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-rxjs": "^5.0.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.5.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.7",
    "@angular/cli": "~15.2.7",
    "@angular/compiler-cli": "^15.2.8",
    "@datorama/akita-ngdevtools": "^7.0.0",
    "@types/jasmine": "~4.3.1",
    "bluebird": "^3.7.2",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-css-order": "^1.3.0",
    "prettier-plugin-organize-attributes": "^0.0.5",
    "pretty-quick": "^3.1.3",
    "typescript": "4.8.2",
    "husky": "^8.0.3"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier -w",
      "pretty-quick --staged",
      "eslint --fix"
    ],
    "src/**/*.html": [
      "prettier -w"
    ],
    "src/**/*.less": [
      "prettier -w"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ASDAlexey/akita"
  },
  "packageManager": "yarn@3.5.0"
}
